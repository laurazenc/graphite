{
    "name": "graphite-core",
    "version": "0.0.0",
    "description": "graphite core logic and components",
    "repository": {
        "type": "git",
        "url": "https://github.com/laurazenc/graphite.git",
        "directory": "packages/core"
    },
    "keywords": [
        "graph",
        "whiteboard",
        "canvas",
        "diagram",
        "workflow",
        "graphite",
        "monorepo",
        "turborepo"
    ],
    "main": "./dist/index.js",
    "source": "./src/index.ts",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/**"
    ],
    "sideEffects": false,
    "license": "MIT",
    "scripts": {
        "dev": "tsc -w",
        "build": "tsc",
        "lint": "eslint \"src/**/*.ts*\"",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
        "test": "jest --watchAll",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "zustand": "4.3.3"
    },
    "devDependencies": {
        "@types/jest": "29.2.2",
        "@types/node": "18.11.7",
        "eslint": "8.26.0",
        "eslint-config-custom": "*",
        "jest": "29.2.2",
        "ts-jest": "29.0.3",
        "ts-node": "10.9.1",
        "tsconfig": "*",
        "tsup": "6.3.0",
        "typescript": "4.8.4"
    },
    "publishConfig": {
        "access": "public"
    }
}
