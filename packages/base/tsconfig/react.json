{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "./common.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["dom", "esnext"],
    "module": "esnext",
    "target": "ESNext",
    "outDir": "dist",
    "downlevelIteration": true,
    "noEmit": true
  }
}